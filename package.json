{
	"name": "tiptap-commonmark",
	"version": "0.2.1",
	"description": "A starter-kit for the tiptap editor, based on CommonMark",
	"type": "module",
	"main": "index.js",
	"module": "./index.js",
	"types": "index.d.ts",
	"keywords": [
		"tiptap",
		"tiptap-editor",
		"tiptap-extension",
		"commonmark",
		"markdown"
	],
	"scripts": {
		"types": "tsc index.js --declaration --allowJs --emitDeclarationOnly",
		"prepack": "npm run types"
	},
	"files": [
		"index.js",
		"index.d.ts"
	],
	"author": "Binyamin Aron Green (https://binyam.in)",
	"license": "MIT",
	"dependencies": {
		"@tiptap/extension-blockquote": "^2.9.1",
		"@tiptap/extension-bold": "^2.9.1",
		"@tiptap/extension-bullet-list": "^2.9.1",
		"@tiptap/extension-code": "^2.9.1",
		"@tiptap/extension-code-block": "^2.9.1",
		"@tiptap/extension-hard-break": "^2.9.1",
		"@tiptap/extension-heading": "^2.9.1",
		"@tiptap/extension-horizontal-rule": "^2.9.1",
		"@tiptap/extension-image": "^2.9.1",
		"@tiptap/extension-italic": "^2.9.1",
		"@tiptap/extension-link": "^2.9.1",
		"@tiptap/extension-list-item": "^2.9.1",
		"@tiptap/extension-ordered-list": "^2.9.1",
		"@tiptap/extension-paragraph": "^2.9.1",
		"@tiptap/extension-text": "^2.9.1"
	},
	"devDependencies": {
		"@tiptap/core": "^2.9.1",
		"@tiptap/extension-code-block-lowlight": "^2.9.1",
		"typescript": "^5.6.3"
	},
	"peerDependencies": {
		"@tiptap/core": "^2.9.1",
		"@tiptap/extension-code-block-lowlight": "^2.9.1"
	},
	"peerDependenciesMeta": {
		"@tiptap/extension-code-block-lowlight": {
			"optional": true
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/binyamin/tiptap-commonmark.git"
	},
	"bugs": {
		"url": "https://github.com/binyamin/tiptap-commonmark/issues"
	},
	"homepage": "https://github.com/binyamin/tiptap-commonmark#readme"
}
